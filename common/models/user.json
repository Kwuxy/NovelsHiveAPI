      "type": "hasMany",
      "model": "Beta_reader",
      "foreignKey": ""
    },
    "ranks": {
      "type": "hasMany",
      "model": "Rank",
      "foreignKey": ""
    },
    "stories": {
      "type": "hasMany",
      "model": "Story",
      "foreignKey": ""
    },
    "publishedCommentaries": {
      "type": "hasMany",
      "model": "Published_commentary",
      "foreignKey": ""
    },
    "draftCommentaries": {
      "type": "hasMany",
      "model": "Draft_commentary",
      "foreignKey": ""
    }
  },
  "acls": [],
  "methods": {}
